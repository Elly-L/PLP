class Vehicle:
    def move(self):
        pass  # This method will be overridden by subclasses

class Car(Vehicle):
    def move(self):
        print("Driving ğŸš—")

class Plane(Vehicle):
    def move(self):
        print("Flying âœˆï¸")

class Bird:
    def move(self):
        print("Flying ğŸ¦…")

# Function to demonstrate the move action
def demonstrate_move(vehicle):
    vehicle.move()

# Create instances of Car and Plane
car = Car()
plane = Plane()
bird = Bird()

# Demonstrate the move action for each vehicle
demonstrate_move(car)    # Output: Driving ğŸš—
demonstrate_move(plane)  # Output: Flying âœˆï¸
demonstrate_move(bird)   # Output: Flying ğŸ¦…
